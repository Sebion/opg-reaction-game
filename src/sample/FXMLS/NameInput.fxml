<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.NameInputController">
   <children>
      <TextField fx:id="nicknameTextField" layoutX="204.0" layoutY="180.0" prefHeight="40.0" prefWidth="193.0" promptText="Jano">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="playButton" defaultButton="true" layoutX="258.0" layoutY="251.0" mnemonicParsing="false" onAction="#on_button_play_pressed" prefHeight="40.0" prefWidth="85.0" text="Play">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="253.0" layoutY="131.0" text="Nickname :">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
