<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.ScoreboardController">
   <children>
      <Button fx:id="backButton" layoutX="14.0" layoutY="340.0" mnemonicParsing="false" onAction="#on_back_button_pressed" prefHeight="46.0" prefWidth="94.0" text="Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TableView fx:id="scoreTable" layoutX="157.0" layoutY="43.0" prefHeight="268.0" prefWidth="287.0">
        <columns>
          <TableColumn fx:id="player" prefWidth="172.0" text="Player" />

            <TableColumn fx:id="highScore" prefWidth="120.0" text="Score" />

        </columns>
      </TableView>
   </children>
</AnchorPane>
