<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.ChangePlayerController">
   <children>
      <Button fx:id="backButton" layoutX="14.0" layoutY="340.0" mnemonicParsing="false" onAction="#on_back_button_pressed" prefHeight="46.0" prefWidth="94.0" text="Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="nameField" layoutX="143.0" layoutY="175.0" prefHeight="46.0" prefWidth="314.0" promptText="Default">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Text layoutX="207.0" layoutY="142.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter your name :">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Button fx:id="submitButton" layoutX="240.0" layoutY="237.0" mnemonicParsing="false" onAction="#on_submitButton_pressed" prefHeight="46.0" prefWidth="120.0" text="Submit">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
